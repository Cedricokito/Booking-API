{
  "name": "booking-api",
  "version": "1.0.0",
  "description": "Property booking API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:ci": "jest --ci --runInBand --coverage --detectOpenHandles",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write 'src/**/*.js' 'tests/**/*.js'",
    "prepare": "husky install"
  },
  "keywords": [
    "booking",
    "api",
    "property",
    "rental"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "mongoose": "^8.2.0",
    "rate-limit-mongo": "^2.3.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.7.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^6.7.0",
    "supertest": "^6.3.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed/index.js"
  }
}
